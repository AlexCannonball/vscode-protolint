{
  "name": "vscode-protobuflint",
  "displayName": "protobuflint",
  "description": "Lint your protocol buffers using protolint (https://github.com/yoheimuta/protolint)",
  "license": "MIT",
  "homepage": "https://github.com/jpreese/vscode-protobuflint",
  "version": "1.0.0",
  "publisher": "jpreese",
  "icon": "icon/protobuflint_logo128.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpreese/vscode-protobuflint.git"
  },
  "bugs": {
    "url": "https://github.com/jpreese/vscode-protobuflint/issues"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Linters"
  ],
  "keywords": [
    "proto",
    "protocol buffer",
    "protobuf",
    "lint"
  ],
  "activationEvents": [
    "onLanguage:proto3"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.protobuflint",
        "title": "VSCode Protobuf Linter"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "precompile": "rimraf ./out",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && mocha out/test/*.test.js"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "rimraf": "^2.6.3",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
